# Server Configuration
PORT=5000
NODE_ENV=development

# Sogni AI Credentials
SOGNI_USERNAME=your-sogni-username
SOGNI_PASSWORD=your-sogni-password
SOGNI_APP_ID=your-sogni-app-id

# Sogni AI Network (fast or relaxed)
# fast = High-end GPUs, faster generation, higher cost
# relaxed = Mac devices, slower generation, lower cost
SOGNI_NETWORK=fast
